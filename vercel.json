{
  "version": 2,
  "routes": [
    {
      "src": "^/manifest\\.webmanifest$",
      "headers": {
        "Content-Type": "application/manifest+json",
        "Cache-Control": "public, max-age=0, must-revalidate"
      }
    },
    {
      "src": "^/sw\\.js$",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      }
    },
    {
      "src": "^/workbox-(.+)\\.js$",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "^/assets/(.+)\\.js$",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "^/assets/(.+)\\.css$",
      "headers": {
        "Content-Type": "text/css",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "^/assets/(.+)\\.woff2$",
      "headers": {
        "Content-Type": "font/woff2",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "^/images/(.+)\\.(webp|png)$",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "^/sound/(.+)\\.mp3$",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}